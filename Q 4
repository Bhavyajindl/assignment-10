#include <iostream>
#include <unordered_map>
#include <vector>
using namespace std;

int firstNonRepeating(vector<int>& nums) {
    unordered_map<int, int> freq;

    // Count frequencies
    for (int x : nums)
        freq[x]++;

    // Find first element with frequency 1
    for (int x : nums)
        if (freq[x] == 1)
            return x;

    return -1; // if no non-repeating element
}

int main() {
    vector<int> nums = {4, 5, 1, 2, 0, 4};
    cout << firstNonRepeating(nums);
}
